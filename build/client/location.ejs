<% var title = 'Location' %>
<!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title><%= title %></title>

    <!-- Import Google Fonts & Icons -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Gravitas+One|Lato:300,400,700" rel="stylesheet">

    <!-- Import Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
  <link href="/./css/styles.css" rel="stylesheet"></head>

  <body<% if (typeof bodyClass !== 'undefined') { %> class="<%= bodyClass %>"<% } %>>


    <nav class="main-nav">
  <div class="nav-wrapper">
    <a href="/" class="brand-logo">V2</a>
    <ul class="manu-list">
      <% menu.forEach(function(item) { %>
        <li><a href="<%= item.link %>"><%= item.text %></a></li>
      <% }) %>
    </ul>
  </div>
</nav>


    <main class="container">
        <h2 class="place__name"></h2>
        <div class="place__results row"></div>
        <div class="progress">
            <div class="indeterminate"></div>
        </div>
    </main>
    

<% var google = true %>
    <% if (typeof google !== 'undefined' && google) { %>
    <!-- Import Google Places API -->
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCYlbzHkh8NuvIE8NVPdu0K5vgraQuoZbo&libraries=places"></script>
    <% } %>
  <script type="text/javascript" src="/js/vendor.958392c236c835642ac8.js"></script><script type="text/javascript" src="/js/location.926451c6630db57eabfe.js"></script></body>
</html>
