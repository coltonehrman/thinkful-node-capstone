<% var title = 'Location' %>
<!DOCTYPE html>
<html>
  <head>
    <meta name="viewport" content="width=device-width, initial-scale=1" />
    <title><%= title %></title>

    <!-- Import Google Fonts & Icons -->
    <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Playfair+Display|Gravitas+One|Lato:300,400,700" rel="stylesheet">

    <!-- Import Font Awesome -->
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css">
  <link href="/./css/styles.css" rel="stylesheet"></head>

  <body<% if (typeof bodyClass !== 'undefined') { %> class="<%= bodyClass %>"<% } %>>


    <nav class="main-nav">
  <div class="nav-wrapper">
    <a href="/" class="brand-logo">V2</a>
    <ul class="manu-list">
      <% menu.forEach(function(item) { %>
        <li><a href="<%= item.link %>"><%= item.text %></a></li>
      <% }) %>
    </ul>
  </div>
</nav>


    <main class="container">
        <h2 class="place__name"></h2>
        <div class="row">
            <div class="col s12 place__results cards-container"></div>
        </div>
        <div class="progress">
            <div class="indeterminate"></div>
        </div>
    </main>
    
    <input class="is-logged-in" type="hidden" value="<%= (user) ? 'true' : 'false' %>">

<% var google = true %>
    <% if (typeof google !== 'undefined' && google) { %>
    <!-- Import Google Places API -->
    <script type="text/javascript" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCYlbzHkh8NuvIE8NVPdu0K5vgraQuoZbo&libraries=places"></script>
    <% } %>
  <script type="text/javascript" src="/js/vendor.65a6bd547b723874f90a.js"></script><script type="text/javascript" src="/js/location.9b81a1dd9215b206c9d8.js"></script></body>
</html>
